<% layout("layouts/boilerplate") %>
<h1><%= category.name %></h1>
<div class="show-container">
  <p><%= category.description %></p>

</div>
<br>
<div class="product-grid-container">
  <% for (product of products) {%>
  <a href="<%= product.url %>">
    <h3><%= product.name %></h3>
    <h2>$<%= product.price.toFixed(2) %></h2>
  </a>
  <% } %>
</div>